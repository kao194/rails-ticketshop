<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Artist</th>
      <th>Description</th>
      <th>Price (low)</th>
      <th>Price (high)</th>
      <th>Event date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.artist %></td>
        <td><%= event.description %></td>
        <td><%= event.price_low %></td>
        <td><%= event.price_high %></td>
        <td><%= event.event_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>